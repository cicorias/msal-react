{
    "extends":"eslint:recommended",
    "plugins": [
      "eslint-plugin-react"
    ],
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true,
        "modules": true,
        "experimentalObjectRestSpread": true
      }
    },
    "env" : {
      "browser" : true,
      "node" : true,
      "es6" : true,
      "jest": true
    },

    "rules": {
        "no-console":"off",
        "react/jsx-uses-vars": 1,
        "react/jsx-uses-react": 1
    }
}